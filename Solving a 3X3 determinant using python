#programe to solve 3X3 determinant
a,b,c=[],[],[]
for i in range(0,3):
 x=int(input("Enter the elements of row 1->"))
 a.append(x)
for i in range(0,3):
 x=int(input("Enter the elements of row 2->"))
 b.append(x)
for i in range(0,3):
 x=int(input("Enter the elements of row 3->"))
 c.append(x)
print(a)
print(b)
print(c)
print("solution ↓↓")
print(a[0]*(b[1]*c[2]-c[1]*b[2])+(-a[1])*(b[0]*c[2]-b[2]*c[0])+a[2]*(b[0]*c[1]-b[1]*c[0]))
